@page
@model Bus.Pages.AllNetworkModel
@{
}
<h1>All Stations Information</h1>
<div class="network-title">
	<h3>Name</h3>
	<h3>Station ID</h3>
</div>
<div class="all-network">
@for (int i =0; i < Model.bService.st.nbStations; i++){
	<div class="net-item">
	<h4>
		@Model.bService.st.stationList[i].name
	</h4>
	<h5>
		@Model.bService.st.stationList[i].stnID
	</h5>
	</div>
}
</div>